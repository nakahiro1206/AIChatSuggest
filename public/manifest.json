{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "description": "クリックでモーダルを開き、データをfetchします。",
    "host_permissions": [
      "https://with.is/*"
    ], 
    "permissions": [
      "storage",
      "scripting", 
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  